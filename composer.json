{
    "name": "lucid-arch/laravel-microservice",
    "description": "The Laravel Framework implemented using the Lucid Architecture - as a Microservice.",
    "keywords": [
        "framework",
        "laravel",
        "lucid",
        "microservices",
        "scaffold",
        "architecture"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "appstract/laravel-meta": "^2.0",
        "appstract/laravel-opcache": "3.2.1",
        "beyondcode/laravel-self-diagnosis": "^1.4",
        "doctrine/dbal": "^2.10",
        "ellipsesynergie/api-response": "^0.16.0",
        "fideloper/proxy": "~4.0",
        "fruitcake/laravel-cors": "^2.0",
        "kalnoy/nestedset": "^5.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "lucid-arch/laravel-foundation": "5.8.*",
        "neomerx/json-api": "v4.0.1",
        "rap2hpoutre/laravel-log-viewer": "^1.6",
        "sentry/sentry-laravel": "^1.8",
        "spatie/laravel-enum": "^1.6",
        "spatie/laravel-fractal": "^5.7",
        "sven/super-basic-auth": "^2.2",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "emielmolenaar/phpcs-laravel": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "imanghafoori/laravel-microscope": "^1.0",
        "laravel/telescope": "v2.1.7",
        "lucid-arch/laravel-console": "5.8.*",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "v0.4.3",
        "phpmd/phpmd": "^2.8",
        "psalm/plugin-laravel": "^1.4",
        "symfony/css-selector": "~4.0",
        "symfony/dom-crawler": "~4.0",
        "vimeo/psalm": "3.14.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
